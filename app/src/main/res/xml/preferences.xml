    <PreferenceCategory android:title="@string/settings_account">
        <PreferenceScreen
            android:key="@string/key_signout"
            android:summary="@string/settings_summary_signout"
            android:title="@string/settings_signout" />

    </PreferenceCategory>
</PreferenceScreen>